/**welcome.wxss**/
@import '/weui.wxss';

page {
  background-color: pink;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.welcome {
  margin-top: 400rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.logo {
  height: 200rpx;
  width: 200rpx;
  border-radius: 100rpx;
}

.login {
  margin-top: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
}
